#include <ArduinoJson.h>

void setup() {
  Serial.begin(115200);
}

void loop() {
  StaticJsonDocument<20000> doc;
  doc["string"] = "Hello, world!";
  doc["int"] = 4782;
  doc["float"] = 3.14657;
  doc["boolean"] = true;
  doc["null"] = nullptr;
  doc["char"] = 'a';
  doc["byte"] = 0xAB;
  doc["word"] = 0xABCD;
  doc["name"] = "SONU KOL ";
  doc["age"] = 64;
  doc["address"] = " Indian ";
  doc["sensor"] = 230.564767;
  doc["data"][0] = 48.756080;
  doc["weather"]["temp"] = 21.2765;
  // doc["array"] = {1, 2, 3};
  // doc["object"] = {{"key1", "value1"}, {"key2", "value2"}};
  serializeJson(doc, Serial);
  Serial.println();
  delay(50);
}
