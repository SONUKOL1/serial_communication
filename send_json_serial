#include <ArduinoJson.h>

void setup() {
  Serial.begin(115200);
}

void loop() {
  StaticJsonDocument<200> doc;
  doc["string"] = "Hello, SONU!";
  doc["int"] = 4782;
  doc["float"] = 3.14657;
  doc["boolean"] = true;
  // doc["array"] = {1, 2, 3};
  // doc["object"] = {{"key1", "value1"}, {"key2", "value2"}};
  doc["null"] = nullptr;
  doc["char"] = 'a';
  doc["byte"] = 0xAB;
  doc["word"] = 0xABCD;

  serializeJson(doc, Serial);
  Serial.println();
  delay(50);
}
